def assort(digits: list) -> tuple:
    if len(digits) == 0:
        return "invalid input"
    else:
        digits.sort()
        return tuple(digits)
    

userIn = input("Enter digits separated by spaces: ").strip()
try:
    digits = list(map(int, userIn.split()))
    print(digits)
    print(assort(digits))
except ValueError:
    print("invalid input")